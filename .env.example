# FitApp Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# Bot Configuration
BOT_USERNAME=fitness_motivator
BOT_PASSWORD=your_secure_bot_password_here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://fitapp.herringm.com/api/auth/google/callback

# VAPID Keys for Push Notifications
VAPID_PUBLIC_KEY=your_vapid_public_key_here
VAPID_PRIVATE_KEY=your_vapid_private_key_here

# Frontend Environment Variables (Vite requires VITE_ prefix)
VITE_API_URL=https://fitappbackend.herringm.com
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
