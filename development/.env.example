# Development Environment Variables
# Copy this file to .env and fill in your actual values

# Node Environment
NODE_ENV=development

# Database Configuration
MONGO_URI=mongodb://mongoosedb:27017/fitapp

# Bot Configuration
BOT_USERNAME=fitness_motivator
BOT_PASSWORD=your_dev_bot_password_here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://fitappdev.herringm.com/api/auth/google/callback

# Matrix/Chat Configuration
MATRIX_HOMESERVER_URL=http://synapse:8008
MATRIX_SERVER_NAME=fitapp.local

# Push Notifications (VAPID Keys)
# Generate these using: node ../fitapp-backend/scripts/generate-vapid-keys.js
VAPID_PUBLIC_KEY=your_vapid_public_key
VAPID_PRIVATE_KEY=your_vapid_private_key

# Frontend Environment Variables
VITE_API_URL=https://fitappbackenddev.herringm.com
VITE_MATRIX_URL=http://synapse:8008